  "scripts": {
    "node:checker": "node -e \"require('child_process').execSync('npm --version', { stdio: 'inherit' }); require('child_process').execSync('node --version', { stdio: 'inherit' });\"",
    "dreamcare:setup:env": "npm run node:checker && npm run dreamcare:agents-orch:setup",
    "dreamcare:agents-orch:setup": "npx nx run dc-agents-orch:install && python -m pip install -e apps/dreamCare/python/agents-orch",
    "dreamcare:agents-orch:serve": "npx nx run dc-agents-orch:serve",
    "auth:serve": "npx nx run auth:serve",
    "blog:serve": "npx nx run blog:serve",
    "booking:serve": "npx nx run booking:serve",
    "chat:serve": "npx nx run chat:serve",
    "e-commerce:serve": "npx nx run e-commerce:serve",
    "e-prescription:serve": "npx nx run e-prescription:serve",
    "machine:serve": "npx nx run machine:serve",
    "notification:serve": "npx nx run notification:serve",
    "provider-dashboard:serve": "npx nx run provider-dashboard:serve",
    "serve:all": "npx nx run-many --target=serve --parallel=12",
    "serve:teamWorkHealth:website": "nx serve teamWorkHealth-website --port 4202",
    "serve:twh:admin": "nx serve twh-admin --port 4203",
    "serve:dc:admin": "nx serve dc-admin-dashboard --port 4203",
    "twh:setup:env": "npm run node:checker && npm run twh:agents-orch:setup",
    "twh:agents-orch:setup": "npx nx run twh-agents-orch:install && python -m pip install -e apps/teamWorkHealth/python/agents-orch",
    "twh:agents-orch:serve": "npx nx run twh-agents-orch:serve",
    "twh:agents-orch-copilot:serve": "npx nx run twh-agents-orch:copilot-serve",
    "twh:copilot-ui:serve": "npx nx run twh-copilot:dev"
  },
